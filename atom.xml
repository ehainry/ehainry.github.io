<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>Velsheda</title>
 <link href="http://velsheda.lateralis.org/journal/atom.xml" rel="self"/>
 <link href="http://velsheda.lateralis.org/journal/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://velsheda.lateralis.org/journal/</id>
 <author><name>Emmanuel Hainry</name><email>ehainry@laposte.net</email></author>
 {% for post in site.posts %}
 <entry><title>{{ post.title }}</title>
  <link href="http://velsheda.lateralis.org/journal{{ post.url }}"/>
  <updated>{{ post.date | date_to_xmlschema }}</updated>
  <id>http://velsheda.lateralis.org/journal/{{ post.id }}</id>
  <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
</feed>
